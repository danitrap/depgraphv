message( STATUS "* Analizing @ProjectName@.exe..." )
	
include( BundleUtilities )
get_bundle_keys( "@CMAKE_INSTALL_PREFIX@/@INSTALL_PATH@/@ProjectName@.exe" "" "@RUNTIME_LIBS_PATH@" keys )

set( deps "@GraphViz_INSTALL_DIRS@/bin/gvplugin_core.dll" )
list( APPEND deps "@GraphViz_INSTALL_DIRS@/bin/gvplugin_dot_layout.dll" )

#gvplugin_gd.dll
#gvplugin_gdiplus.dll
#gvplugin_neato_layout.dll
#gvplugin_pango.dll

set( QT5 @QT_USE_QT5@ )
if( QT5 )
	list( APPEND deps "@CMAKE_PREFIX_PATH@bin/libEGL.dll" )
endif()

foreach( key ${keys} )
	string( REGEX MATCH "@ProjectName@.exe$" result "${${key}_RESOLVED_ITEM}" )
	if( NOT result )
		list( APPEND deps "${${key}_RESOLVED_ITEM}" )
	endif()
endforeach()

file( INSTALL DESTINATION "@CMAKE_INSTALL_PREFIX@/@INSTALL_PATH@" TYPE FILE FILES ${deps} )